<h1>カテゴリー一覧</h1>
<%= render "menu" %>
<table class="categories">
  <col class="name"/>
  <col class="links"/>
  <% @categories.each do |category| %>
    <tr>
      <td><%= category.name %></td>
      <td><%= category.id %></td>
      <td><%= category.owner_id %></td>
      <td><%= links_for category %></td>
    </tr>
  <% end %>
</table>